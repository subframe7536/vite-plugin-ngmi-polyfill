{
  "name": "@subframe7536/vite-plugin-ngmi-polyfill",
  "description": "forked from vite-plugin-ngmi-polyfill with fixes",
  "version": "0.1.1",
  "author": "subframe7536",
  "repository": "https://github.com/grikomsn/vite-plugin-ngmi-polyfill.git",
